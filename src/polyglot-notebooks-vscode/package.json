{
  "name": "dotnet-interactive-vscode",
  "displayName": "Polyglot Notebooks",
  "description": "%description.package%",
  "icon": "images/icon.png",
  "publisher": "i574n",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "enabledApiProposals": [
    "notebookMessaging"
  ],
  "preview": false,
  "//version": "%description.version%",
  "version": "42.42.42",
  "engines": {
    "vscode": "^1.87.0"
  },
  "bugs": {
    "url": "https://github.com/dotnet/interactive/issues"
  },
  "repository": {
    "url": "https://github.com/dotnet/interactive"
  },
  "categories": [
    "Data Science",
    "Machine Learning",
    "Notebooks",
    "Visualization"
  ],
  "keywords": [
    ".NET",
    "interactive",
    "C#",
    "data science",
    "dotnet",
    "F#",
    "ipynb",
    "Jupyter",
    "notebooks",
    "PowerShell"
  ],
  "activationEvents": [
    "onUri",
    "onNotebook:dotnet-interactive-i574n",
    "onNotebook:dotnet-interactive-i574n-window",
    "onNotebook:jupyter-notebook-i574n",
    "onNotebook:polyglot-notebook-i574n",
    "onNotebook:polyglot-notebook-i574n-window",
    "onNotebook:jupyter-notebook-i574n",
    "onCommand:dotnet-interactive-i574n.acquire",
    "onCommand:polyglot-notebook-i574n.fileNew",
    "onCommand:polyglot-notebook-i574n.newNotebook",
    "onCommand:polyglot-notebook-i574n.openNotebook",
    "onCommand:polyglot-notebook-i574n.saveAsNotebook",
    "onView:polyglot-notebook-i574n-panel-values"
  ],
  "main": "./out/src/vscode-common/extension.js",
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "%description.untrustedWorkspaces%"
    },
    "virtualWorkspaces": true
  },
  "l10n": "./l10n",
  "contributes": {
    "notebooks": [
      {
        "type": "polyglot-notebook-i574n",
        "displayName": "Polyglot Notebook",
        "selector": [
          {
            "filenamePattern": "*.dib"
          }
        ]
      },
      {
        "type": "polyglot-nortebook-window",
        "displayName": "Polyglot Notebook",
        "selector": []
      }
    ],
    "jupyter.kernels": [
      {
        "title": ".NET Interactive (C#)",
        "defaultlanguage": "polyglot-notebook-i574n"
      },
      {
        "title": ".NET Interactive (F#)",
        "defaultlanguage": "polyglot-notebook-i574n"
      },
      {
        "title": ".NET Interactive (PowerShell)",
        "defaultlanguage": "polyglot-notebook-i574n"
      }
    ],
    "configuration": {
      "title": "Polyglot Notebook",
      "properties": {
        "polyglot-notebook-i574n.suppressPromptToSaveDefaults": {
          "type": "boolean",
          "default": false,
          "description": "%description.suppressPromptToSaveDefaults%"
        },
        "polyglot-notebook-i574n.defaultNotebookExtension": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "enum": [
            ".dib",
            ".ipynb"
          ],
          "enumDescriptions": [
            "%description.configuration.useDib%",
            "%description.configuration.useIpynb%"
          ]
        },
        "polyglot-notebook-i574n.defaultNotebookLanguage": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "%description.defaultNotebookLanguage%",
          "enum": [
            "csharp",
            "fsharp",
            "spiral",
            "html",
            "javascript",
            "kql",
            "markdown",
            "mermaid",
            "pwsh",
            "sql"
          ],
          "enumDescriptions": [
            "C#",
            "F#",
            "Spiral",
            "HTML",
            "JavaScript",
            "KQL",
            "Markdown",
            "Mermaid",
            "PowerShell",
            "SQL"
          ]
        },
        "polyglot-notebook-i574n.kernelEnvironmentVariables": {
          "type": "object",
          "default": {},
          "description": "%description.kernelEnvironmentVariables%"
        },
        "polyglot-notebook-i574n.languageServiceDelay": {
          "type": "number",
          "default": 500,
          "description": "%description.languageServiceDelay%"
        },
        "polyglot-notebook-i574n.logLevel": {
          "type": "string",
          "default": "Error",
          "enum": [
            "Info",
            "Warn",
            "Error",
            "None"
          ],
          "enumDescriptions": [
            "%description.logLevel.info%",
            "%description.logLevel.warning%",
            "%description.logLevel.error%",
            "%description.logLevel.disabled%"
          ]
        },
        "dotnet-interactive-i574n.kernelTransportArgs": {
          "type": "array",
          "default": [
            "{dotnet_path}",
            "tool",
            "run",
            "dotnet-interactive-i574n",
            "--",
            "[vscode]",
            "stdio",
            "--working-dir",
            "{working_dir}"
          ],
          "description": "%description.kernelTransportArgs%"
        },
        "dotnet-interactive-i574n.notebookParserArgs": {
          "type": "array",
          "default": [
            "{dotnet_path}",
            "tool",
            "run",
            "dotnet-interactive-i574n",
            "--",
            "notebook-parser"
          ]
        },
        "dotnet-interactive-i574n.kernelTransportWorkingDirectory": {
          "type": "string",
          "default": "{global_storage_path}",
          "description": "%description.kernelTransportWorkingDirectory%"
        },
        "dotnet-interactive-i574n.interactiveToolSource": {
          "type": "string",
          "default": "https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-tools/nuget/v3/index.json",
          "description": "%description.interactiveToolSource%"
        },
        "dotnet-interactive-i574n.minimumDotNetSdkVersion": {
          "type": "string",
          "default": "8.0",
          "description": "%description.minimumDotNetSdkVersion%"
        },
        "dotnet-interactive-i574n.requiredInteractiveToolVersion": {
          "type": "string",
          "default": "0.1.0",
          "description": "%description.requiredInteractiveToolVersion%"
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "polyglot-notebook-i574n-panel",
          "title": "Polyglot Notebook",
          "icon": "resources/icons/notebook.svg"
        }
      ]
    },
    "views": {
      "polyglot-notebook-i574n-panel": [
        {
          "type": "webview",
          "id": "polyglot-notebook-i574n-panel-values",
          "name": "Variables",
          "icon": "resources/icons/notebook.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "dotnet-interactive-i574n.acquire",
        "title": "%command.acquire%"
      },
      {
        "command": "polyglot-notebook-i574n.openNotebook",
        "title": "%command.openNotebook%"
      },
      {
        "command": "polyglot-notebook-i574n.saveAsNotebook",
        "title": "%command.saveAsNotebook%"
      },
      {
        "command": "polyglot-notebook-i574n.fileNew",
        "enablement": "false",
        "title": "%command.fileNew%",
        "shortTitle": "Polyglot Notebook"
      },
      {
        "command": "polyglot-notebook-i574n.newNotebook",
        "title": "%command.newNotebook%"
      },
      {
        "command": "polyglot-notebook-i574n.newNotebookNoDefaults",
        "title": "%command.newNotebookNoDefaults%"
      },
      {
        "command": "polyglot-notebook-i574n.setNewNotebookDefaults",
        "title": "%command.setNewNotebookDefaults%"
      },
      {
        "command": "polyglot-notebook-i574n.restartCurrentNotebookKernel",
        "title": "%command.restartCurrentNotebookKernel%"
      },
      {
        "command": "polyglot-notebook-i574n.stopCurrentNotebookKernel",
        "title": "%command.stopCurrentNotebookKernel%"
      },
      {
        "command": "polyglot-notebook-i574n.stopAllNotebookKernels",
        "title": "%command.stopAllNotebookKernels%"
      },
      {
        "command": "polyglot-notebook-i574n.shareValueWith",
        "title": "%command.shareValueWith%"
      },
      {
        "command": "polyglot-notebook-i574n.notebookEditor.restartKernel",
        "title": "%command.restartKernel%",
        "icon": {
          "light": "resources/light/restart-kernel.svg",
          "dark": "resources/dark/restart-kernel.svg"
        }
      },
      {
        "command": "polyglot-notebook-i574n.notebookEditor.openValueViewer",
        "title": "%command.openValueViewer%",
        "icon": "$(variable-group)"
      }
    ],
    "keybindings": [
      {
        "command": "polyglot-notebook-i574n.newNotebook",
        "key": "ctrl+alt+shift+n",
        "mac": "cmd+alt+shift+n"
      }
    ],
    "languages": [
      {
        "id": "polyglot-notebook-i574n",
        "aliases": [
          "Code"
        ]
      }
    ],
    "menus": {
      "file/newFile": [
        {
          "command": "polyglot-notebook-i574n.fileNew",
          "group": "notebook"
        }
      ],
      "notebook/toolbar": [
        {
          "command": "polyglot-notebook-i574n.notebookEditor.restartKernel",
          "group": "navigation/execute@1",
          "when": "notebookKernel =~ /polyglot-notebook-i574n/ && isWorkspaceTrusted"
        },
        {
          "command": "polyglot-notebook-i574n.notebookEditor.openValueViewer",
          "group": "navigation@1",
          "when": "notebookKernel =~ /polyglot-notebook-i574n/ && isWorkspaceTrusted"
        }
      ]
    },
